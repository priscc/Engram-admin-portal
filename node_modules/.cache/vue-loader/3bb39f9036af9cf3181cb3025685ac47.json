\"d-flex flex-column\">\n        <div class=\"d-flex\">\n          <v-card\n            class=\"d-block pt-5 \"\n            width=\"7vw\"\n            height=\"5vw\"\n            color=\"grey lighten-2\"\n          >\n            <v-img :src=\"url\"></v-img>\n          </v-card>\n\n          <div style=\"width: 17vw\">\n            <v-file-input\n              class=\"pt-10\"\n              @change=\"Preview_image\"\n              v-model=\"image\"\n              accept=\"image/*\"\n              prepend-icon=\"none\"\n              append-icon=\"attach_file\"\n              placeholder=\"Upload Profile thumbnail\"\n            >\n            </v-file-input>\n          </div>\n        </div>\n        <div class=\"d-flex flex-row align-center justify-center pt-8\">\n          <v-btn\n            class=\"white--text\"\n            width=\"180\"\n            color=\"#3891A6\"\n            :disabled=\"!checkfield\"\n            elevation=\"2\"\n            @click=\"clearWorkFields\"\n            >Reset</v-btn\n          >\n\n          <v-btn\n            class=\"white--text ml-3\"\n            width=\"180\"\n            color=\"#3891A6\"\n            elevation=\"2\"\n            @click=\"submitNewWork\"\n            >+ Add Term</v-btn\n          >\n        </div>\n      </div>\n    </div>\n    <div></div>\n    <div class=\"d-flex justify-center pt-6\">\n      <div class=\"pt-1 pr-2\" style=\"font-size: 18px\">Sort by:</div>\n      <div style=\"width: 15vw\">\n        <v-select\n          v-model=\"selectedSort\"\n          :items=\"sortBy\"\n          :menu-props=\"{ top: false, offsetY: false }\"\n          background-color=\"grey lighten-2\"\n          outlined\n          dense\n        >\n        </v-select>\n      </div>\n    </div>\n    <div class=\"d-flex flex-column align-center mx-auto\">\n      <div v-for=\"work in currentTopicWorks\" :key=\"work.id\">\n        <Artifact class=\"mx-2 my-2\" v-bind:work=\"work\" />\n      </div>\n    </div>\n  </v-app>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\nimport { mapFields } from \"vuex-map-fields\";\nimport Artifact from \"./Artifact/Artifact.vue\";\n\nexport default {\n  data() {\n    return {\n      term: null,\n      def: null,\n      sortBy: [\"Term: Ascending\", \"Term: Descending\"],\n      selectedSort: \"Term: Ascending\",\n      image: null,\n      url: null,\n    };\n  },\n  components: { Artifact },\n  computed: {\n    checkfield() {\n      return this.title || this.caption;\n    },\n    ...mapGetters(\"works\", [\"currentTopicWorks\"]),\n    ...mapFields(\"works\", [\"currentWork.title\", \"currentWork.caption\"]),\n  },\n  methods: {\n    Preview_image: () => {},\n    ...mapActions(\"works\", [\n      \"fetchWorks\",\n      \"submitNewWork\",\n      \"clearWorkId\",\n      \"clearWorkFields\",\n    ]),\n  },\n  created() {\n    this.fetchWorks();\n    this.clearWorkId();\n    this.clearWorkFields();\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n#imageThumbnail {\n  width: 15vw;\n  height: 10vw;\n  background-color: #5b6368;\n}\n#nav {\n  a {\n    padding: 4px 24px;\n    text-decoration: none;\n    color: #ffffff;\n    font-weight: lighter;\n    background-color: #5b6368;\n\n    &.router-link-exact-active {\n      background-color: #3891a6;\n    }\n  }\n}\n</style>\n"]}]}