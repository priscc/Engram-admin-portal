=\"border: 1px solid #979797;margin: -1px 0px 0px -1px\"\n        color=\"#D8D8D8\"\n      >\n        <youtube\n          :video-id=\"video.url\"\n          :player-width=\"111\"\n          :player-height=\"72\"\n        ></youtube>\n        <div v-if=\"showDiv\" style=\"padding: 8px; text-align: left\">\n          Preview of current video:\n        </div>\n      </v-card>\n      <div class=\"d-flex flex-column\" style=\"position: absolute; right: 0px\">\n        <div id=\"cardbtn1\">\n          <!-- Modal when editing a video -->\n          <v-dialog v-model=\"editDialog\" persistent max-width=\"500\">\n            <template v-slot:activator=\"{ on, attrs }\">\n              <span class=\"material-icons\">\n                <v-icon\n                  class=\"d-flex\"\n                  size=\"20\"\n                  color=\"#3891A6\"\n                  style=\"cursor: pointer\"\n                  @click=\"editVideo(video)\"\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                >\n                  create\n                </v-icon>\n              </span>\n            </template>\n            <v-card>\n              <v-card-title class=\"headline d-flex justify-center\">\n                <div class=\"mt-4\">\n                  Editing a video\n                </div>\n              </v-card-title>\n              <div class=\"d-flex justify-center\">\n                <v-card\n                  class=\" d-flex\"\n                  width=\"111px\"\n                  height=\"80px\"\n                  outlined\n                  style=\"border: 1px solid #979797\"\n                  color=\"#D8D8D8\"\n                >\n                  <youtube\n                    v-if=\"showYoutube\"\n                    :video-id=\"currentVideoURL\"\n                    :player-width=\"111\"\n                    :player-height=\"78\"\n                  ></youtube>\n                  <div v-if=\"showDiv\" style=\"padding: 8px; text-align: left\">\n                    Preview of current video:\n                  </div>\n                </v-card>\n                <div class=\" d-flex pl-8\">\n                  <v-form ref=\"form\" lazy-validation>\n                    <v-text-field\n                      class=\"mt-5\"\n                      v-model=\"title\"\n                      placeholder=\"Add Video Title\"\n                      required\n                      dense\n                    ></v-text-field>\n                    <v-text-field\n                      class=\"mt-n3\"\n                      v-model=\"videoURL\"\n                      placeholder=\"Video Link\"\n                      required\n                      dense\n                    ></v-text-field>\n                  </v-form>\n                </div>\n              </div>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn text @click=\"cancelHandler\">\n                  Cancel\n                </v-btn>\n\n                <!-- Final confirmation on saving edited term -->\n                <v-btn color=\"#3891A6\" text @click=\"submitEditVideoHandler\">\n                  Save\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n          <!-- Modal  -->\n\n          <!-- Modal when deleting a video -->\n          <v-dialog v-model=\"delDialog\" persistent max-width=\"250\">\n            <template v-slot:activator=\"{ on, attrs }\">\n              <span class=\"material-icons\">\n                <v-icon\n                  class=\"d-flex\"\n                  size=\"20\"\n                  color=\"#3891A6\"\n                  style=\"cursor: pointer\"\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                >\n                  disabled_by_default\n                </v-icon>\n              </span>\n            </template>\n            <v-card>\n              <v-card-title class=\"headline\">\n                <div>Are you sure you want to delete {{ video.title }}?</div>\n              </v-card-title>\n              <v-card-text\n                >Once {{ video.title }} is deleted you can never retrieve\n                it</v-card-text\n              >\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn text @click=\"delDialog = false\">\n                  Cancel\n                </v-btn>\n\n                <!-- Final confirmation on deleting video -->\n                <v-btn\n                  color=\"red darken-1\"\n                  text\n                  @click=\"deleteVideoHandler(video)\"\n                >\n                  Yes, delete\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n          <!-- Modal  -->\n        </div>\n      </div>\n      <div class=\"font-weight-regular d-flex align-center ml-8\">\n        {{ video.title }}\n      </div>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\nimport { mapFields } from \"vuex-map-fields\";\n\nexport default {\n  data() {\n    return {\n      editDialog: false,\n      delDialog: false,\n    };\n  },\n  name: \"Video\",\n  props: [\"video\"],\n  computed: {\n    showYoutube() {\n      if (this.currentVideoURL == \"\") {\n        return false;\n      } else {\n        return true;\n      }\n    },\n    showDiv() {\n      if (this.currentVideoURL == \"\") {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    ...mapGetters(\"videos\", [\"currentVideoURL\"]),\n    ...mapFields(\"videos\", [\"currentVideoResource.title\", \"videoURL\"]),\n  },\n  methods: {\n    ...mapActions(\"videos\", [\n      \"editVideo\",\n      \"clearVideoForm\",\n      \"submitEditVideo\",\n      \"deleteVideo\",\n      \"fetchVideos\",\n    ]),\n    ready(event) {\n      this.player = event.target;\n    },\n    stop() {\n      this.player.stopVideo();\n    },\n    pause() {\n      this.player.pauseVideo();\n    },\n    async submitEditVideoHandler() {\n      await this.submitEditVideo();\n      this.editDialog = false;\n      await this.fetchVideos(\"topic\");\n      console.log(\"Submitted Edit Video\");\n    },\n    async cancelHandler() {\n      this.editDialog = false;\n      await this.fetchVideos(\"topic\");\n      this.clearVideoForm();\n      console.log(\"Cancelled Edit\");\n    },\n    async deleteVideoHandler(video) {\n      await this.deleteVideo(video);\n      this.delDialog = false;\n      await this.fetchVideos(\"topic\");\n      console.log(\"Deleted \" + video.title);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped></style>\n"]}]}