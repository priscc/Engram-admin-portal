\n          style=\"text-align: left; font-size: 24px\"\n        >\n          Preview of Event thumbnail\n        </div> -->\n        <v-img\n          v-if=\"event.thumbURL\"\n          :src=\"event.thumbURL\"\n          width=\"198px\"\n          height=\"137px\"\n        ></v-img>\n      </v-card>\n      <div class=\"d-flex flex-column\" style=\"position: absolute; right: 0px\">\n        <div id=\"cardbtn1\">\n          <span class=\"material-icons\">\n            <v-icon\n              class=\"d-flex\"\n              size=\"30\"\n              color=\"#3891A6\"\n              style=\"cursor: pointer\"\n              @click=\"editEventHandler(event)\"\n            >\n              create\n            </v-icon>\n          </span>\n\n          <!-- Modal when deleting an event -->\n          <v-dialog v-model=\"dialog\" persistent max-width=\"250\">\n            <template v-slot:activator=\"{ on, attrs }\">\n              <span class=\"material-icons\">\n                <v-icon\n                  class=\"d-flex\"\n                  size=\"30\"\n                  color=\"#3891A6\"\n                  style=\"cursor: pointer\"\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                >\n                  disabled_by_default\n                </v-icon>\n              </span>\n            </template>\n            <v-card>\n              <v-card-title class=\"headline\">\n                <div>Are you sure you want to delete {{ event.title }}?</div>\n              </v-card-title>\n              <v-card-text\n                >Once {{ event.title }} is deleted you can never retrieve\n                it</v-card-text\n              >\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn text @click=\"dialog = false\">\n                  Cancel\n                </v-btn>\n\n                <!-- Final confirmation on deleting event -->\n                <v-btn\n                  color=\"red darken-1\"\n                  text\n                  @click=\"deleteEventHandler(event)\"\n                >\n                  Yes, delete\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n          <!-- Modal  -->\n        </div>\n      </div>\n      <div class=\"d-flex flex-column justify-center pl-6\">\n        <div class=\"d-flex\" style=\"font-size: 24px\">\n          {{ event.title }}\n        </div>\n        <div class=\"d-flex\" style=\"font-size: 18px\">\n          {{ event.startDate.date }} - {{ event.endDate.date }}\n        </div>\n      </div>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from \"vuex\";\n\nexport default {\n  data() {\n    return {\n      dialog: false,\n      url: null\n    };\n  },\n  name: \"Event\",\n  props: [\"event\"],\n  methods: {\n    ...mapActions(\"events\", [\"editEvent\", \"deleteEvent\", \"fetchEventsList\"]),\n    deleteEventHandler(event) {\n      this.deleteEvent(event);\n      this.dialog = false;\n      console.log(\"Deleted \" + event.title);\n      console.log(event);\n    },\n    editEventHandler(event) {\n      this.editEvent(event);\n      setTimeout(\n        () => this.$router.push({ name: \"EventGeneral\", path: \"/general\" }),\n        200\n      );\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped></style>\n"]}]}