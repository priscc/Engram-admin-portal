       outlined\n        style=\"border: 1px solid #979797;margin: -1px 0px 0px -1px\"\n        color=\"#D8D8D8\"\n        ><div\n          class=\"d-flex align-center pl-5\"\n          style=\"text-align: left; font-size: 24px\"\n        >\n          Preview of Artifact\n        </div></v-card\n      >\n      <div class=\"d-flex flex-column\" style=\"position: absolute; right: 0px\">\n        <div id=\"cardbtn1\">\n          <!-- Modal when editing an artifact -->\n          <v-dialog v-model=\"saveDialog\" persistent max-width=\"500\">\n            <template v-slot:activator=\"{ on, attrs }\">\n              <span class=\"material-icons\">\n                <v-icon\n                  class=\"d-flex\"\n                  size=\"30\"\n                  color=\"#3891A6\"\n                  style=\"cursor: pointer\"\n                  @click=\"editWork(work)\"\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                >\n                  create\n                </v-icon>\n              </span>\n            </template>\n            <v-card>\n              <v-card-title class=\"headline d-flex justify-center\">\n                <div class=\"mt-4\">\n                  Editing an Artifact\n                </div>\n              </v-card-title>\n              <div class=\"d-flex flex-column mx-auto\" style=\"width: 400px\">\n                <v-col class=\"pt-10\" cols=\"15\">\n                  <v-text-field\n                    label=\"Artifact:\"\n                    background-color=\"grey lighten-2\"\n                    v-model=\"title\"\n                    outlined\n                    dense\n                  ></v-text-field>\n                </v-col>\n                <v-col class=\"mt-n7\" cols=\"15\">\n                  <v-textarea\n                    v-model=\"caption\"\n                    outlined\n                    name=\"input-7-4\"\n                    label=\"Caption:\"\n                    background-color=\"grey lighten-2\"\n                    height=\"7vw\"\n                  ></v-textarea>\n                </v-col>\n              </div>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn text @click=\"cancelHandler\">\n                  Cancel\n                </v-btn>\n\n                <!-- Final confirmation on saving edited artifact -->\n                <v-btn color=\"#3891A6\" text @click=\"submitEditWorktHandler\">\n                  Save\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n          <!-- Modal  -->\n\n          <!-- Modal when deleting a term -->\n          <v-dialog v-model=\"delDialog\" persistent max-width=\"260\">\n            <template v-slot:activator=\"{ on, attrs }\">\n              <span class=\"material-icons\">\n                <v-icon\n                  class=\"d-flex\"\n                  size=\"30\"\n                  color=\"#3891A6\"\n                  style=\"cursor: pointer\"\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                >\n                  disabled_by_default\n                </v-icon>\n              </span>\n            </template>\n            <v-card>\n              <v-card-title class=\"headline\">\n                <div>\n                  Are you sure you want to delete this artifact?\n                </div>\n              </v-card-title>\n              <v-card-text\n                >Once the artifact is deleted you can never retrieve\n                it</v-card-text\n              >\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn text @click=\"delDialog = false\">\n                  Cancel\n                </v-btn>\n\n                <!-- Final confirmation on deleting term -->\n                <v-btn\n                  color=\"red darken-1\"\n                  text\n                  @click=\"deleteWorkHandler(work)\"\n                >\n                  Yes, delete\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n          <!-- Modal  -->\n        </div>\n      </div>\n      <div class=\"d-flex flex-column justify-center pl-6\">\n        <div class=\"d-flex\" style=\"font-size: 24px\">\n          Historical Figure {{ work.title }}\n        </div>\n        <div class=\"d-flex\" style=\"font-size: 18px; text-align: left\">\n          {{ work.caption }}\n        </div>\n      </div>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from \"vuex\";\nimport { mapFields } from \"vuex-map-fields\";\n\nexport default {\n  data() {\n    return {\n      saveDialog: false,\n      delDialog: false,\n    };\n  },\n  name: \"Artifact\",\n  props: [\"work\"],\n  computed: {\n    ...mapFields(\"works\", [\"currentWork.title\", \"currentWork.caption\"]),\n  },\n  methods: {\n    ...mapActions(\"works\", [\n      \"editWork\",\n      \"submitEditWork\",\n      \"fetchWorks\",\n      \"clearWorkId\",\n      \"clearWorkFields\",\n      \"deleteWork\",\n    ]),\n    submitEditWorktHandler() {\n      this.submitEditWork();\n      this.saveDialog = false;\n      console.log(\"Submitted Edit\");\n    },\n    cancelHandler() {\n      this.clearWorkFields();\n      this.clearWorkId();\n      this.saveDialog = false;\n    },\n    deleteWorkHandler(work) {\n      this.deleteWork(work);\n      this.delDialog = false;\n      console.log(\"Deleted\");\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped></style>\n"]}]}