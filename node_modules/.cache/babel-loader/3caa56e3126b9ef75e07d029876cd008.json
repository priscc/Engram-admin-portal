                :player-height=\"78\"\n              ></youtube>\n              <div v-if=\"showDiv\" style=\"padding: 8px; text-align: left\">\n                Preview of current video:\n              </div>\n            </v-card>\n            <div class=\"d-flex flex-row\" style=\"position: absolute; right: 0px\">\n              <div>\n                <span class=\"material-icons\">\n                  <v-icon\n                    class=\"d-flex\"\n                    size=\"20\"\n                    color=\"#3891A6\"\n                    style=\"cursor: pointer\"\n                    v-on:click=\"clearVideoForm\"\n                  >\n                    refresh\n                  </v-icon>\n                </span>\n              </div>\n              <div>\n                <span class=\"material-icons\">\n                  <v-icon\n                    class=\"d-flex\"\n                    size=\"20\"\n                    color=\"#3891A6\"\n                    style=\"cursor: pointer\"\n                    v-on:click=\"submitNewVideoHandler(parentType)\"\n                  >\n                    add_box\n                  </v-icon>\n                </span>\n              </div>\n            </div>\n            <div class=\" d-flex justify-center align-center mx-auto\">\n              <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                <v-text-field\n                  class=\"mt-5\"\n                  v-model=\"title\"\n                  placeholder=\"Add Video Title\"\n                  required\n                  dense\n                ></v-text-field>\n                <v-text-field\n                  class=\"mt-n3\"\n                  v-model=\"videoURL\"\n                  placeholder=\"Video Link\"\n                  required\n                  dense\n                ></v-text-field>\n              </v-form>\n            </div>\n          </v-card>\n        </div>\n      </div>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\nimport { mapFields } from \"vuex-map-fields\";\n\nimport Video from \"./Video/Video.vue\";\nexport default {\n  name: \"Videos\",\n  components: { Video },\n  data() {\n    return {\n      valid: true,\n      image: null,\n      parentType: \"topic\",\n    };\n  },\n  computed: {\n    ...mapGetters(\"videos\", [\"currentVideos\", \"currentVideoURL\"]),\n    ...mapFields(\"videos\", [\"currentVideoResource.title\", \"videoURL\"]),\n    showYoutube() {\n      if (this.currentVideoURL == \"\") {\n        return false;\n      } else {\n        return true;\n      }\n    },\n    showDiv() {\n      if (this.currentVideoURL == \"\") {\n        return true;\n      } else {\n        return false;\n      }\n    },\n  },\n  methods: {\n    ready(event) {\n      this.player = event.target;\n    },\n    stop() {\n      this.player.stopVideo();\n    },\n    pause() {\n      this.player.pauseVideo();\n    },\n    ...mapActions(\"videos\", [\n      \"fetchVideos\",\n      \"submitNewVideo\",\n      \"clearVideoForm\",\n    ]),\n    validate() {\n      this.$refs.form.validate();\n      console.log(\"Submitted\");\n      alert(\"Submitted\");\n    },\n    async submitNewVideoHandler(type) {\n      await this.submitNewVideo(type);\n      await this.fetchVideos(type);\n    },\n  },\n  created() {\n    this.clearVideoForm();\n    this.fetchVideos(\"topic\");\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped></style>\n"],"sourceRoot":"src/components/ModifyTopic/Forms/Resources"}]}