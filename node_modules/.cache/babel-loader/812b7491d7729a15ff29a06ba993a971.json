A;AAAA,iBAAA,GAJA;;AAAA;AAAA;AAAA;;AAAA;AAWA,gBAAA,OAAA,CAAA,GAAA,CAAA,eAAA;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,oBAAA;AACA,gBAAA,MAAA,CAAA,QAAA,GACA,iJADA;AAEA,gBAAA,UAAA,uEAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iCACA,MAAA,CAAA,cAAA,EADA;;AAAA;AAEA,0BAAA,MAAA,CAAA,UAAA,GAAA,KAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iBAAA,IAGA,IAHA,CAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBA,KAzEA;AA0EA;AACA;AACA;AACA;AACA;AACA,IAAA,aA/EA,2BA+EA;AACA,WAAA,eAAA;AACA,WAAA,WAAA;AACA,WAAA,UAAA,GAAA,KAAA;AACA,KAnFA;AAoFA,IAAA,iBApFA,6BAoFA,IApFA,EAoFA;AACA,WAAA,UAAA,CAAA,IAAA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA;AAxFA,IApBA;AA8GA,EAAA,OA9GA,qBA8GA;AACA,SAAA,YAAA;AACA;AAhHA,CAAA","sourcesContent":["<template>\n  <div class=\"d-flex pt-4 justify-center align-center\">\n    <v-card\n      class=\"d-flex\"\n      width=\"580px\"\n      height=\"139px\"\n      outlined\n      style=\"border: 1px solid #979797\"\n    >\n      <v-card\n        class=\"d-flex\"\n        width=\"250px\"\n        height=\"139px\"\n        outlined\n        style=\"border: 1px solid #979797;margin: -1px 0px 0px -1px\"\n        color=\"#D8D8D8\"\n      >\n        <!-- <div\n          class=\"d-flex align-center pl-5\"\n          style=\"text-align: left; font-size: 24px\"\n        >\n          Preview of Artifact\n        </div> -->\n        <v-img\n          v-if=\"work.thumbURL\"\n          :src=\"work.thumbURL\"\n          width=\"198px\"\n          height=\"137px\"\n        ></v-img>\n      </v-card>\n      <div class=\"d-flex flex-column\" style=\"position: absolute; right: 0px\">\n        <div id=\"cardbtn1\">\n          <!-- Modal when editing an artifact -->\n          <v-dialog v-model=\"saveDialog\" persistent max-width=\"500\">\n            <template v-slot:activator=\"{ on, attrs }\">\n              <span class=\"material-icons\">\n                <v-icon\n                  class=\"d-flex\"\n                  size=\"30\"\n                  color=\"#3891A6\"\n                  style=\"cursor: pointer\"\n                  @click=\"editWork(work)\"\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                >\n                  create\n                </v-icon>\n              </span>\n            </template>\n            <v-card>\n              <v-card-title class=\"headline d-flex justify-center\">\n                <div class=\"mt-4\">\n                  Editing an Artifact\n                </div>\n              </v-card-title>\n              <div class=\"d-flex flex-column mx-auto\" style=\"width: 400px\">\n                <v-col class=\"pt-10\" cols=\"15\">\n                  <v-text-field\n                    label=\"Artifact:\"\n                    background-color=\"grey lighten-2\"\n                    v-model=\"title\"\n                    outlined\n                    dense\n                  ></v-text-field>\n                </v-col>\n                <v-col class=\"mt-n7\" cols=\"15\">\n                  <v-textarea\n                    v-model=\"caption\"\n                    outlined\n                    name=\"input-7-4\"\n                    label=\"Caption:\"\n                    background-color=\"grey lighten-2\"\n                    height=\"7vw\"\n                  ></v-textarea>\n                </v-col>\n                <div class=\"d-flex pl-3 mt-n4 \">\n                  <v-card\n                    class=\"d-block\"\n                    height=\"80px\"\n                    width=\"10vw\"\n                    color=\"grey lighten-2\"\n                  >\n                    <v-img\n                      v-if=\"url\"\n                      :src=\"url\"\n                      height=\"80px\"\n                      width=\"10vw\"\n                    ></v-img>\n                  </v-card>\n\n                  <div style=\"width: 17vw\">\n                    <input\n                      class=\"pl-4\"\n                      ref=\"input1\"\n                      type=\"file\"\n                      @change=\"previewImage\"\n                    />\n                  </div>\n                </div>\n              </div>\n\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn text @click=\"cancelHandler\">\n                  Cancel\n                </v-btn>\n\n                <!-- Final confirmation on saving edited artifact -->\n                <v-btn color=\"#3891A6\" text @click=\"submitEditWorkHandler()\">\n                  Save\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n          <!-- Modal  -->\n\n          <!-- Modal when deleting a term -->\n          <v-dialog v-model=\"delDialog\" persistent max-width=\"260\">\n            <template v-slot:activator=\"{ on, attrs }\">\n              <span class=\"material-icons\">\n                <v-icon\n                  class=\"d-flex\"\n                  size=\"30\"\n                  color=\"#3891A6\"\n                  style=\"cursor: pointer\"\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                >\n                  disabled_by_default\n                </v-icon>\n              </span>\n            </template>\n            <v-card>\n              <v-card-title class=\"headline\">\n                <div>\n                  Are you sure you want to delete this artifact?\n                </div>\n              </v-card-title>\n              <v-card-text\n                >Once the artifact is deleted you can never retrieve\n                it</v-card-text\n              >\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn text @click=\"delDialog = false\">\n                  Cancel\n                </v-btn>\n\n                <!-- Final confirmation on deleting term -->\n                <v-btn\n                  color=\"red darken-1\"\n                  text\n                  @click=\"deleteWorkHandler(work)\"\n                >\n                  Yes, delete\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n          <!-- Modal  -->\n        </div>\n      </div>\n      <div class=\"d-flex flex-column justify-center pl-6\">\n        <div class=\"d-flex\" style=\"font-size: 24px\">\n          Historical Figure {{ work.title }}\n        </div>\n        <div class=\"d-flex\" style=\"font-size: 18px; text-align: left\">\n          {{ work.caption }}\n        </div>\n      </div>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\nimport { mapFields } from \"vuex-map-fields\";\nimport firebase from \"firebase\";\n\nexport default {\n  data: () => {\n    return {\n      saveDialog: false,\n      delDialog: false,\n      url: null,\n      imageData: null,\n    };\n  },\n  name: \"Artifact\",\n  props: [\"work\"],\n  computed: {\n    ...mapFields(\"works\", [\n      \"currentWork.title\",\n      \"currentWork.caption\",\n      \"currentWork.thumbFile\",\n      \"currentWork.thumbURL\",\n    ]),\n    ...mapGetters(\"works\", [\"currentWork\"]),\n  },\n  methods: {\n    ...mapActions(\"works\", [\n      \"editWork\",\n      \"submitEditWork\",\n      \"fetchWorks\",\n      \"clearWorkId\",\n      \"clearWorkFields\",\n      \"deleteWork\",\n    ]),\n    mountPreview() {\n      this.url = this.currentWork.thumbURL;\n    },\n    previewImage(e) {\n      this.uploadValue = 0;\n      console.log(e.target.files[0]);\n      let file = e.target.files[0];\n      let fileSize = file.size / 1024 / 1024;\n      if (fileSize > 2) {\n        e.preventDefault();\n        alert(\"File is over 2mb\");\n      } else {\n        this.url = URL.createObjectURL(file);\n        this.imageData = e.target.files[0];\n        this.thumbFile = file.name;\n        console.log(this.thumbFile);\n        console.log(this.imageData);\n      }\n    },\n    async onUpload() {\n      const storageRef = firebase\n        .storage()\n        .ref(`${this.imageData.name}`)\n        .put(this.imageData);\n      storageRef.on(\n        `state_changed`,\n        (snapshot) => {\n          this.uploadValue =\n            (snapshot.bytesTransferred / snapshot.totalBytes) * 100;\n          console.log(\"step2\");\n        },\n        (error) => {\n          console.log(error.message);\n        },\n        async () => {\n          this.uploadValue = 100;\n          await storageRef.snapshot.ref.getDownloadURL().then((url) => {\n            // this.img1 = url;\n            // console.log(this.img1);\n            this.thumbURL = url;\n          });\n        }\n      );\n    },\n    async submitEditWorkHandler() {\n      if (this.imageData) {\n        console.log(\"with image\");\n        let imgPromise = Promise.resolve(this.onUpload());\n        await imgPromise.then(async () => {\n          setTimeout(async () => {\n            await this.submitEditWork();\n            this.saveDialog = false;\n          }, 2000);\n        });\n      } else {\n        console.log(\"without image\");\n        this.thumbFile = \"placeHolderImg.png\";\n        this.thumbURL =\n          \"https://firebasestorage.googleapis.com/v0/b/study-bites-1.appspot.com/o/placeHolderImg.png?alt=media&token=38eced07-54a4-4b3a-b2f9-49fa8e01da63\";\n        setTimeout(async () => {\n          await this.submitEditWork();\n          this.saveDialog = false;\n        }, 2000);\n      }\n    },\n    //     submitEditWorktHandler() {\n    //       this.submitEditWork();\n    //       this.saveDialog = false;\n    //       console.log(\"Submitted Edit\");\n    //     },\n    cancelHandler() {\n      this.clearWorkFields();\n      this.clearWorkId();\n      this.saveDialog = false;\n    },\n    deleteWorkHandler(work) {\n      this.deleteWork(work);\n      this.delDialog = false;\n      console.log(\"Deleted\");\n    },\n  },\n  created() {\n    this.mountPreview();\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped></style>\n"],"sourceRoot":"src/components/ModifyContent/ContentDetails/PrimarySources/Artifact"}]}